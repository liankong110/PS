            item.EndTime = currentRow.find(".c").val();
            goodsList.push(item);

        });
        console.log(goodsList);

        var url = "/PS/Save";
        $.ajax({
            data: { '__RequestVerificationToken': $('input[name=__RequestVerificationToken]').val(), Pro_PS: JSON.stringify(Pro_PS), Pro_PSDetail: JSON.stringify(goodsList) },
            url: url,
            type: 'post',
            async: false,
