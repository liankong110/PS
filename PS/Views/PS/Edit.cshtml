            item.GoodNo = cells[2].innerText;
            item.GoodName = cells[3].innerText;
            item.STypeString = cells[4].innerText;
            item.ShipTo = cells[5].innerText;
            item.PackNum = cells[6].innerText;
            item.ChanNeng = cells[7].innerText;
