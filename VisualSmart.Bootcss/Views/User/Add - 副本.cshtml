@{
    ViewBag.Title = "Add";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model VisualSmart.Domain.SetUp.UserDomain
<div class="container-fluid">
    <div class="form" style="max-width:300px;margin:0 auto;">
        <div class="form-group">
            <label for="loginId">用户名</label>
            @Html.TextBoxFor(t => t.loginId, new { @class = "form-control" })
        </div>
        <div class="form-group">
            <label for="loginPwd">登录密码</label>
            @Html.TextBoxFor(t => t.loginPwd, new { @class = "form-control" })
        </div>
        <div class="form-group">
            <label for="Name">真实姓名</label>
            @Html.TextBoxFor(t => t.Name, new { @class = "form-control" })
        </div>
        <div class="form-group">
            <label for="Phone">手机</label>
            @Html.TextBoxFor(t => t.Phone, new { @class = "form-control" })
        </div>
        <div class="form-group">
            <label for="Email">Email</label>
            @Html.TextBoxFor(t => t.Email, new { @class = "form-control" })
        </div>
        <div class="form-group">
            <label for="Email">Email</label>
            @Html.TextBoxFor(t => t.Email, new { @class = "form-control" })
        </div>
        <div class="form-group">
            <label for="Position">职位</label>
            @Html.TextBoxFor(t => t.Position, new { @class = "form-control" })
        </div>
        <div class="form-group">
            <label for="Leader">所属上级</label>
            @Html.TextBoxFor(t => t.Leader, new { @class = "form-control" })
        </div>
        <div class="form-group">
            <label for="Company">所属公司</label>
            @Html.TextBoxFor(t => t.Company, new { @class = "form-control" })
        </div>
        <div class="form-group">
            <input type="submit" class="btn btn-primary" value="添加保存" />
        </div>
    </div>
</div>